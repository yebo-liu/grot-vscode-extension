{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["\"", "\""],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "\"", "close": "\"" },
    { "open": "(", "close": ")" },
    { "open": "\"\"\"", "close": "\"\"\"" }
  ],
  "surroundingPairs": [
    ["\"", "\""],
    ["(", ")"]
  ],
  "folding": {
    "markers": {
      "start": "^>\\s*@MPRS",
      "end": "^(?=>\\s*@MPRS|$)"
    }
  },
  "wordPattern": "@?[A-Za-z0-9_:]+"
}
